<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00CCFC"
    tools:context=".NumberOfHoles">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="75dp"
        android:fontFamily="@font/arial_rounded_mt_bold"
        android:text="How many holes?"
        android:textColor="#000000"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10">

        <Button
            android:id="@+id/nineHolesSelectedButton"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:fontFamily="@font/arial_rounded_mt_bold"
            android:text="9"
            android:textColor="#000000"
            android:textSize="34sp"
            app:backgroundTint="#00FFCE"
            app:cornerRadius="75dp"
            app:strokeColor="#000000"
            app:strokeWidth="2sp" />

        <Space
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <Button
            android:id="@+id/eighteenHolesSelectedButton"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:fontFamily="@font/arial_rounded_mt_bold"
            android:text="18"
            android:textColor="#000000"
            android:textSize="34sp"
            app:backgroundTint="#00FFCE"
            app:cornerRadius="75dp"
            app:strokeColor="#000000"
            app:strokeWidth="2sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/otherHolesSelectionView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:alpha="0.0"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10">

        <Button
            android:id="@+id/decreaseHoleCountButton"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:fontFamily="@font/arial_rounded_mt_bold"
            android:text="-"
            android:textColor="#000000"
            android:textSize="60sp"
            app:backgroundTint="#00FFCE"
            app:cornerRadius="75dp"
            app:strokeColor="#000000"
            app:strokeWidth="2sp" />

        <TextView
            android:id="@+id/holeCountLabel"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:clickable="false"
            android:enabled="false"
            android:fontFamily="@font/arial_rounded_mt_bold"
            android:foregroundGravity="center"
            android:gravity="center"
            android:linksClickable="false"
            android:longClickable="false"
            android:text="3"
            android:textColor="#000000"
            android:textSize="60sp" />

        <Button
            android:id="@+id/increaseHoleCountButton"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:fontFamily="@font/arial_rounded_mt_bold"
            android:text="+"
            android:textColor="#000000"
            android:textSize="60sp"
            app:backgroundTint="#00FFCE"
            app:cornerRadius="75dp"
            app:strokeColor="#000000"
            app:strokeWidth="2sp" />
    </LinearLayout>

    <Button
        android:id="@+id/customHoleNumberSelectedButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:alpha="0.0"
        android:text="Next"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="34sp"
        app:backgroundTint="#00FFCE"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:strokeColor="#000000"
        app:strokeWidth="2sp" />

    <Button
        android:id="@+id/otherNumberOfHolesButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:alpha="1.0"
        android:text="Other"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="34sp"
        app:backgroundTint="#00FFCE"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:strokeColor="#000000"
        app:strokeWidth="2sp" />

    <FrameLayout
        android:id="@+id/adViewFrameLayoutNumPlayers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>